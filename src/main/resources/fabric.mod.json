{
	"schemaVersion": 1,
	"id": "fishack",
	"version": "${version}",
	"name": "fishack",
	"description": "Fishack. This is not a real hack client but actualy a client/mod that has feature from a hack client that are fun and are not considered a hack. Only use in allowed environments. ",
	"authors": [
		"FISH"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/fishack/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.fishack.FishackMod"
		],
		"client": [
			"com.fishack.FishackModClient"
		]
	},
	"mixins": [
		"fishack.mixins.json",
		{
			"config": "fishack.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}